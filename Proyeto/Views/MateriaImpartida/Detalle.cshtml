@model Proyeto.Models.MateriaImpartidaModel

@{
    ViewData["Title"] = "Detalle de Materia Impartida";
}

<link rel="stylesheet" href='~/css/StyleSheet1.css' />
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
<link rel="stylesheet" href='~/css/footerFelcidades.css' />



<div class="row">
    <h4>Materia Impartida</h4>
    <hr />
    <dl class="row">
       
       
        @Html.HiddenFor(model => model.IdMateria)
       
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Matricula)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Matricula)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CarreraModel.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CarreraModel.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.MateriaAdmin.NombreMat)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MateriaAdmin.NombreMat)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Grupo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Grupo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaCuatri)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaCuatri)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.UrlDocumento)
        </dt>
        <dd class = "col-sm-10">
            <a target="_blank" href="@Model.UrlDocumento">Descargar Documento</a>
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdAutor1)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdAutor1)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Editar", new { id = Model.IdMateria }) |
    <a asp-action="Index">Regresar a la lista</a>
</div>
